<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cn21.FrequencyControl.dao.UserDao">

   <select id="selectUser" parameterType="com.cn21.FrequencyControl.module.User" resultType="com.cn21.FrequencyControl.module.User">  
            select * from user where username=#{username} and password=#{password}  
        </select>  
        <insert id="insertUser" parameterType="com.cn21.FrequencyControl.module.User" flushCache="true">  
            insert into user(username,password) values(#{username},#{password})  
        </insert>  
        <update id="updateUser" parameterType="com.cn21.FrequencyControl.module.User">  
            update user set username=#{username} where user_id=#{user_id}  
        </update>  
        <delete id="deleteUser" parameterType="int">  
            delete from user where user_id=#{user_id}  
        </delete>  
</mapper>
